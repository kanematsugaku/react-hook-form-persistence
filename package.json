{
  "name": "react-hook-form-persistence",
  "version": "0.0.8",
  "description": "A very simple library for persisting react-hook-form.",
  "license": "MIT",
  "main": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kanematsugaku/react-hook-form-persistence.git"
  },
  "author": "Gaku Kanematsu",
  "contributors": [
    "Kazuma Nakagawa"
  ],
  "keywords": [
    "react-hook-form",
    "persist",
    "persistence"
  ],
  "peerDependencies": {
    "react": "^17.0.0",
    "react-hook-form": "^7.0.0"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/eslint": "^7.28.1",
    "@types/prettier": "^2.4.1",
    "@types/react": "^17.0.29",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.4.1",
    "typescript": "^4.4.4"
  },
  "scripts": {
    "build": "tsc",
    "link": "npm link && cd app && npm link react-hook-form-persistence",
    "unlink": "cd app && npm unlink react-hook-form-persistence && cd .. && npm unlink react-hook-form-persistence && npm uninstall --g react-hook-form-persistence",
    "prepublishOnly": "npm run build",
    "lint:tsc": "tsc --noEmit",
    "lint:eslint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "fix:eslint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "lint:prettier": "prettier --check src",
    "fix:prettier": "prettier --write src",
    "lint:all": "npm run lint:tsc && npm run lint:eslint && npm run lint:prettier",
    "ncu": "npx npm-check-updates -i -u",
    "typesync": "npx typesync",
    "depcheck": "npx depcheck"
  }
}
